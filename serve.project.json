{
	"name": "syn_defaults-dev",
	"servePort": 34875,
	"globIgnorePaths": [
		"**/package.json",
		"**/tsconfig.json",
		"**/tests/test.project.json"
	],
	"tree": {
		"$className": "DataModel",
		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",

			"rbxts_include": {
				"$path": "include",

				"node_modules": {
					"$className": "Folder",

					"@rbxts": { 
						"$path": "node_modules/@rbxts"
					},
					"@rbxts-js": {
						"$path": "node_modules/@rbxts-js"
					},
					"@flamework": {
						"$path": "node_modules/@flamework"
					}
				}
			},

			"Plugin": { "$path": "out" }
		}
	}
}