{
	"name": "syn_defaults_plugin",
	"servePort": 34875,
	"globIgnorePaths": [
		"**/package.json",
		"**/tsconfig.json",
		"**/tests",
		"**/stories"
	],
	"tree": {
		"$path": "out",

		"rbxts_include": {
			"$path": "include",

			"node_modules": {
				"$className": "Folder",

				"@rbxts": { 
					"$path": "node_modules/@rbxts"
				},
				"@rbxts-js": {
					"$path": "node_modules/@rbxts-js"
				},
				"@flamework": {
					"$path": "node_modules/@flamework"
				}
			}
		}
	}
}